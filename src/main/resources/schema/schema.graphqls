type Query {
    users: [User]
    user(id:String!): User
    username(name:String!): [User]
}

type Mutation {
    addUser(name:String!, birthDay: String, created: String, updated: String):User
    updateUserName(id:String!, name:String): User
    deleteUser(id: String!): Boolean
}


type User {
    id: String
    name: String
    birthDay: String
    created: String
    updated: String
}